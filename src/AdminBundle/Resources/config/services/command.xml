<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="langland.command.admin_user_create"
                class="Library\Command\AdminUserCreateCommand" public="true">

            <argument type="service" id="doctrine.orm.default_entity_manager" />
            <argument type="service" id="security.password_encoder" />

            <tag name="console.command" />
        </service>

        <service
                id="langland.command.language_images_add_command"
                class="AdminBundle\Command\LanguageImagesAddingCommand" public="true">

            <argument>%image_upload%</argument>
            <argument type="service" id="langland.learning_metadata.repository.language" />

            <tag name="console.command" />
        </service>
    </services>
</container>