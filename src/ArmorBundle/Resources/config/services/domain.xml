<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service
                id="armor.domain.language_session"
                class="Armor\Domain\LanguageSessionLogic" public="true">

            <argument type="service" id="armor.repository.user" />
            <argument type="service" id="public_api.repository.learning_user" />
            <argument type="service" id="armor.repository.language_session" />
            <argument type="service" id="armor.communication.public_api_language_communicator" />
        </service>

        <service
                id="armor.domain.user"
                class="Armor\Domain\UserLogic" public="true">

        </service>

    </services>
</container>