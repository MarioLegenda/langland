<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="langland.data_provider.language"
                class="Tests\TestLibrary\DataProvider\LanguageDataProvider">

            <argument type="service" id="langland.learning_metadata.repository.language" />
            <argument type="service" id="langland.data_provider.image" />
        </service>

        <service
                id="langland.data_provider.course"
                class="Tests\TestLibrary\DataProvider\CourseDataProvider">

            <argument type="service" id="langland.learning_metadata.repository.course" />
            <argument type="service" id="langland.data_provider.language" />
        </service>

        <service
                id="langland.data_provider.lesson"
                class="Tests\TestLibrary\DataProvider\LessonDataProvider">

            <argument type="service" id="langland.learning_metadata.repository.lesson" />
            <argument type="service" id="langland.data_provider.course" />

        </service>

        <service
                id="langland.data_provider.user"
                class="TestLibrary\DataProvider\UserDataProvider">

            <argument type="service" id="armor.repository.user" />

        </service>

        <service
                id="langland.data_provider.image"
                class="Tests\TestLibrary\DataProvider\ImageDataProvider">

        </service>

    </services>
</container>
