<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="learning_system.business.controller.initial_data_creation"
                public="true"
                class="LearningSystem\Business\Controller\InitialDataCreationController">

            <argument type="service" id="learning_system.business.implementation.initial_data_creation" />
        </service>

        <service
                id="learning_system.business.controller.learning_metadata"
                class="LearningSystem\Business\Controller\LearningMetadataController"
                public="true">

            <argument type="service" id="learning_system.business.implementation.learning_metadata" />
        </service>
    </services>
</container>