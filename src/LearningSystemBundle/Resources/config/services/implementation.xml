<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="learning_system.business.implementation.initial_data_creation"
                public="true"
                class="LearningSystem\Business\Implementation\InitialDataCreationImplementation">

            <argument type="service" id="library.api_sdk" />
            <argument type="service" id="learning_system.game_worker.initial_system_game_worker" />
            <argument type="service" id="public_api.learning_system.data_provider.game_provider" />
        </service>

        <service
                id="learning_system.business.implementation.learning_metadata"
                public="true"
                class="LearningSystem\Business\Implementation\LearningMetadataImplementation">

            <argument type="service" id="learning_system.repository.learning_metadata" />
            <argument type="service" id="learning_system.provider.learning_user_provider" />
            <argument type="service" id="library.api_sdk" />
        </service>
    </services>
</container>