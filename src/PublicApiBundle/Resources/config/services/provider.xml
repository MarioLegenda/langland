<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="langland.public_api.question_answers_application_provider"
                class="PublicApi\LearningSystem\QuestionAnswersApplicationProvider">

            <argument type="service" id="langland.public_api.learning_user_provider" />
        </service>

        <service
                id="langland.public_api.learning_user_provider"
                class="PublicApi\LearningUser\Infrastructure\Provider\LearningUserProvider">

            <argument type="service" id="security.token_storage" />
        </service>
    </services>
</container>